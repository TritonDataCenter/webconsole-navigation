{
  consul: 'localhost:8500',
  jobs: [
    {
      name: 'consul-agent',
      exec: ['/usr/local/bin/consul', 'agent',
                  '-data-dir=/data',
                  '-config-dir=/config',
                  '-log-level=err',
                  '-rejoin',
                  '-retry-join', '{{ .CONSUL | default "consul" }}',
                  '-retry-max', '20',
                  '-retry-interval', '5s'],
      restarts: 'unlimited',
      health: {
        exec: "curl -so /dev/null http://127.0.0.1:8500/v1/status/leader",
        interval: 10,
        ttl: 25,
      }
    },
    {
      name: 'webconsole-navigation',
      port: {{.PORT}},
      exec: 'node server.js',
      health: {
        exec: '/usr/bin/curl -o /dev/null --fail -s http://127.0.0.1:{{.PORT}}/{{ .HEALTH_ENDPOINT | default "health" }}',
        interval: 5,
        ttl: 5
      },
      restarts: 'unlimited'
    }
  ],
  watches: []
}
